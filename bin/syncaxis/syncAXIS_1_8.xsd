        <xs:annotation>
          <xs:documentation xml:lang="EN">List of correction files.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Alignment" type="cfg:TransformationType">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Transformation applied to the scan device motion. Should be used for the alignment of the scan device to another reference system (e.g. stage or another scan device).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BasePartDisplacement" type="cfg:TransformationType">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Additional transformation applied to scan device motion to compensate for the disorientation of a work piece. As the stages motion component cannot be transformed accordingly for mutliple parts, it's contribution is also compensated by the scanner.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Name" type="cfg:ScanDeviceNameType">
      <xs:annotation>
        <xs:documentation xml:lang="EN">Name of scanhead device (non editable). Must be unique.</xs:documentation>
        </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="TransformationType">
    <xs:all>
      <xs:element name="Matrix" type="cfg:DoubleMatrix2x2Type">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Transformation matrix for the respective component.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Offset">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Offset of the respective component.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:attribute name="X" type="xs:double" default="0" />
          <xs:attribute name="Y" type="xs:double" default="0" />
          <xs:attribute name="Unit" fixed="mm" type="xs:string" use="optional" />
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="RTCConfigType">
    <xs:all>
      <xs:element name="BoardIdentificationMethod" default="UseFirstFound">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Determines what RTC to be used for this syncAXIS control instance. If there is only one, with UseFirstFound the first RTC found by the RTC DLL is used. Using BySerialNumber a specific RTC can by selected by its serial number.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="UseFirstFound" />
            <xs:enumeration value="BySerialNumber" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ProgramFileDirectory" type="xs:string" minOccurs="0" default="">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Directory to the RTC Firmware (program files: RTC6RBF.rbf, RTC6DAT.dat and RTC6OUT.out).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Boards" type="cfg:BoardsType">
        <xs:annotation>
          <xs:documentation xml:lang="EN">List of boards to be used.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EthSearch" type="cfg:EthSearchType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Enables and manages the ethernet options.</xs:documentation>          
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="ScanDeviceConfigType">
    <xs:all>
      <xs:element name="DynamicLimits" type="cfg:ScanHeadDynamicLimitsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Determines the dynamic limits of the scan device.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CalculationDynamics" type="cfg:ScanHeadCalculationDynamicsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Determines calculation dynamics of the scan device.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FieldLimits" type="cfg:RectangleType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Defines the image field limit of the scan device.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MonitoringLevel" default="Position">
         <xs:annotation>
          <xs:documentation xml:lang="en">Defines the monitoring level for the reaction defined in DynamicViolationReaction. Possible values are Deactivated, Position, Velocity, Acceleration, and Jerk. Lower orders are always included (i.e. Velocity tracks position and velocity breaches etc.)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Deactivated" />
            <xs:enumeration value="Position" />
            <xs:enumeration value="Velocity" />
            <xs:enumeration value="Acceleration" />
            <xs:enumeration value="Jerk" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="FocalLength" type="cfg:Length" minOccurs="0" default="100">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            The focal length of the scan system. Depends on the optics used. syncAXIS control uses these values to plan trajectories for the operation modes "ScannerOnly" and "ScannerAndStage".</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Delay" type="cfg:DurationMax1s" minOccurs="0" default="0.00125">
        <xs:annotation>
          <xs:documentation xml:lang="en">Delay of the scan system. excelliSCAN 14 typically have a delay of 1.25 ms.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ScanDeviceList" type="cfg:ScanDeviceListType">
        <xs:annotation>
          <xs:documentation xml:lang="en">List of scan devices.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DefaultCorrectionFile" minOccurs="0" default="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Determines which correction file from the list of Correction Files will be used within this syncAXIS control instance.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:nonNegativeInteger">
            <xs:maxInclusive value="3" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="LaserPinOutType">
    <xs:attribute name="Format" fixed="Bitpattern" type="xs:string" use="optional" />
    <xs:attribute name="Value" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="3" />
          <xs:pattern value="(0x[0-3]|[0-3])" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="UnitIntervalType">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0.0" />
      <xs:maxInclusive value="1.0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AnalogOutType">
    <xs:annotation>
      <xs:documentation xml:lang="EN">AnalogOutType AnalogOutType.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="Format" fixed="Factor" type="xs:string" use="optional" />
    <xs:attribute name="Value" type="cfg:UnitIntervalType" use="required" />
  </xs:complexType>
  <xs:complexType name="DigitalOutType">
    <xs:attribute name="Format" fixed="Bitpattern" type="xs:string" use="optional" />
    <xs:attribute name="Value" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="6" />
          <xs:minLength value="1" />
          <xs:pattern value="(0x[0-9a-fA-F]+|[0-9]+)" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="Mask" use="optional">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:maxLength value="6" />
          <xs:minLength value="1" />
          <xs:pattern value="(0x[0-9a-fA-F]+|[0-9]+)" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="RtcCommandSeqType">
    <xs:sequence>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Delay" type="cfg:Duration" />
        <xs:element name="SetLaserPinOut" type="cfg:LaserPinOutType" />
        <xs:element name="SetAnalogOut1" type="cfg:AnalogOutType" />
        <xs:element name="SetAnalogOut2" type="cfg:AnalogOutType" />
        <xs:element name="SetExt1DigitalOut" type="cfg:DigitalOutType" />
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="IOInitialisationConfigType">
    <xs:all>
      <xs:element name="DefaultOutputs" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Configures the state to which the various peripheral RTC output ports will be set at the beginning and end of a job, and after job execution halted either via software command,  external stop or via automatic stop triggered by head power NOK status or stage status NOK.  If a specific port is not explicitly set the output will remain unchanged on these occasions.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:all>
            <xs:element name="LaserPinOut" type="cfg:LaserPinOutType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="EN">Laser Pin Out bitmask.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AnalogOut1" type="cfg:AnalogOutType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="EN">Analog Out 1 value.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AnalogOut2" type="cfg:AnalogOutType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="EN">Analog Out 2 value.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="LaserInitSequence" type="cfg:RtcCommandSeqType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Command sequence which will executed during initialization immediately after a successful stage handshake and setting of the default outputs.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaserShutdownSequence" type="cfg:RtcCommandSeqType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Command sequence which will be executed on syncAXIS instance destruction.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:attributeGroup name="VersionGroup">
    <xs:attribute name="Version" fixed="1.8" use="required" />
  </xs:attributeGroup>
  <xs:element name="Configuration">
    <xs:complexType>
      <xs:all>
        <xs:element name="GeneralConfig" type="cfg:GeneralConfigType" />
        <xs:element name="RTCConfig" type="cfg:RTCConfigType" />
        <xs:element name="ScanDeviceConfig" type="cfg:ScanDeviceConfigType" />
        <xs:element name="LaserConfig" type="cfg:LaserConfigType" />
        <xs:element name="IOConfig" type="cfg:IOInitialisationConfigType" minOccurs="0" />
        <xs:element name="TrajectoryConfig" type="cfg:TrajectoryConfigType" />
        <xs:element name="StageConfig" type="cfg:StageConfigType" />
        <xs:element name="MotionDecompositionConfig" type="cfg:MotionDecompositionConfigType" />
        <xs:element name="SystemConfig" minOccurs="0">
          <xs:complexType mixed="true">
            <xs:sequence>
              <xs:any maxOccurs="unbounded" minOccurs="0" processContents="skip" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:all>
      <xs:attributeGroup ref="cfg:VersionGroup" />
    </xs:complexType>
  </xs:element>
  
  
</xs:schema>