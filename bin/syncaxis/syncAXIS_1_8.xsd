<?xml version="1.0" encoding="utf-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="syncAXIS" version="1.8" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cfg="syncAXIS">
  <xs:complexType name="SimulationConfigType">
    <xs:all>
      <xs:element name="SimulationMode" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="EN">The SimulationMode parameter determines whether the simulation mode is activated or not. If true, the syncAXIS control instance will not communicate with any Hardware device (except the XL SCAN Dongle) but will, instead, write the trajectory planning results into a text file.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SimOutputFileDirectory" type="xs:string" minOccurs="0" default="">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Directory path for the simulation output files.The name of each simulation outpur file differs according to the signature described in the syncAXIS API manual.}</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BinaryOutput" type="xs:boolean" minOccurs="0" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Activates writing binary values in the simulation output files.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisableFileOutput" type="xs:boolean" minOccurs="0" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Deactivates the writing of simulation output files. Simulations will get much faster. This mode is recommended if not interested in the whole trajectory but the job characteristics only.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="LogConfigType">
    <xs:all>
      <xs:element name="LogfilePath" type="xs:string" default="Error.log">
        <xs:annotation>
          <xs:documentation xml:lang="EN">File path of the log file that is created by the syncAXIS control instance when logging is enabled. If this file already exists, logging information will be appended to the file.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Loglevel" default="Warn">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Sets the logging level. Possible values are "Error", "Warn", "Info", "Debug" and "Trace". Loglevel "Debug" and "Trace" are only available with XL SCAN - Service Dongles.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Error" />
            <xs:enumeration value="Warn" />
            <xs:enumeration value="Info" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="EnableConsoleLogging" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Determines whether console logging will be activated.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EnableFilelogging" type="xs:boolean" default="true">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Determines whether file logging will be activated.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MaxLogfileSize" type="xs:positiveInteger" default="5242880" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">The maximum size of a single log file.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MaxBackupFileCount" type="xs:nonNegativeInteger" default="10" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Determines how many files to be backed up (by adding a counting integer to the file name) before the actual file will be continuously overwritten.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="IPAddressType">
    <xs:restriction base="xs:string">
      <xs:pattern value="(([01][0-9][0-9]|2[0-4][0-9]|25[0-5]|[0-9][0-9]|[0-9])\.){3}([01][0-9][0-9]|2[0-4][0-9]|25[0-5]|[0-9][0-9]|[0-9])" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="GeneralConfigType">
    <xs:all>
      <xs:element name="ACSController" type="cfg:IPAddressType" minOccurs="0" default="127.0.0.1">
        <xs:annotation>
          <xs:documentation xml:lang="EN">IP address of the ACS Controller EtherCAT network.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="InitialOperationMode" default="ScannerAndStage">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Sets the initial operation mode.  Possible values are ScannerAndStage, ScannerOnly, and StageOnly.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="ScannerAndStage" />
            <xs:enumeration value="ScannerOnly" />
            <xs:enumeration value="StageOnly" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="InitialListHandlingMode" default="ReturnAtOnce">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Sets the initial list handling mode. This setting specifies how buffering of the slsc_list_*-functions is handled. Possible values are "ReturnAtOnce" and "RepeatWhileBufferFull". "RepeatWhilePredicate" is only possible via syncAXIS-API.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="ReturnAtOnce" />
            <xs:enumeration value="RepeatWhileBufferFull" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DynamicViolationReaction" default="WarningOnly">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines which reaction occurs automatically, as soon as a certain monitoring criterion (dynamic limits, working field limits) is violated. Possible values are WarningOnly, AbortImmediately, and StopAndReport.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="WarningOnly" />
            <xs:enumeration value="AbortImmediately" />
            <xs:enumeration value="StopAndReport" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LogConfig" type="cfg:LogConfigType"> 
        <xs:annotation>
          <xs:documentation xml:lang="EN">Parameters used for enabling logging and the amount of log message to be recorded.  Warning: the amount of logging may affect the execution.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BaseDirectoryPath" type="xs:string" minOccurs="0" default="">
        <xs:annotation>
          <xs:documentation xml:lang="EN">This value functions as a macro which can be inserted as '[BaseDirectoryPath]\' with certain configuration file tags (SimOutputFileDirectory, LogfilePath, ProgramFileDirectory. Its purpose is to make folderpath specifications shorter and easier to change.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SimulationConfig" type="cfg:SimulationConfigType">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Parameters used for managing the simulation mode of the syste</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="LaserPortType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="LaserOnSignal" />
      <xs:enumeration value="Laser1Signal" />
      <xs:enumeration value="Laser2Signal" />
      <xs:enumeration value="FirstPulseKillerSignal" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="LaserPortsConfigType">
    <xs:all>
      <xs:element name="LaserOn" type="cfg:LaserPortType" default="LaserOnSignal">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Specifies the laser signal for output channel Laser On.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Laser1" type="cfg:LaserPortType" default="Laser1Signal">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Specifies the laser signal for output channel Laser 1.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Laser2" type="cfg:LaserPortType" default="Laser2Signal">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Specifies the laser signal for output channel Laser 2.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:simpleType name="NonNegDouble">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0.0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NonNegMin1over64usDouble">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="15.625e-9" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Max1sDouble">
    <xs:restriction base="xs:double">
      <xs:maxInclusive value="1.0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NonNegMax1sDouble">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0.0" />
      <xs:maxInclusive value="1.0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="Duration">
    <xs:simpleContent>
      <xs:extension base="cfg:NonNegDouble">
        <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="DurationMin1over64us">
    <xs:simpleContent>
      <xs:extension base="cfg:NonNegMin1over64usDouble">
        <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="DurationMax1s">
    <xs:simpleContent>
      <xs:extension base="cfg:NonNegMax1sDouble">
        <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="TimeShift">
    <xs:simpleContent>
      <xs:extension base="xs:double">
        <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="TimeShiftMax1s">
    <xs:simpleContent>
      <xs:extension base="cfg:Max1sDouble">
        <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="NonZeroDouble">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0.00001" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="NonZeroLength">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="mm" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Length">
    <xs:simpleContent>
      <xs:extension base="xs:double">
        <xs:attribute name="Unit" fixed="mm" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Velocity">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="mm/s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ZeroVelocity">
    <xs:simpleContent>
      <xs:extension base="cfg:NonNegDouble">
        <xs:attribute name="Unit" fixed="mm/s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Acceleration">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="mm/s^2" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Jerk">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="mm/s^3" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="RadVelocity">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="rad/s" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="RadAcceleration">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="rad/s^2" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="RadJerk">
    <xs:simpleContent>
      <xs:extension base="cfg:NonZeroDouble">
        <xs:attribute name="Unit" fixed="rad/s^3" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Angle">
    <xs:simpleContent>
      <xs:extension base="xs:double">
        <xs:attribute name="Unit" fixed="rad" type="xs:string" use="optional" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="PulseConfigType">
    <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
    <xs:attribute name="HalfPeriod" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="EN">This parameter determines the pulse repetition rate. The Pulse Half Period is half the time between the two consecutive rising edges of the Laser 1 (and/or Laser 2) signal pulses.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:double">
          <xs:minInclusive value="0.0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="PulseLength" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="EN">The Pulse Length is the width of the Laser 1 (and/or Laser 2) signal pulses.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:double">
          <xs:minInclusive value="0.0" />
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="DataPointLengthVelType">
    <xs:attribute name="Length" type="xs:double" />
    <xs:attribute name="Velocity" type="xs:double" />
  </xs:complexType>
  <xs:complexType name="CharacteristicCurveLengthVelType">
    <xs:annotation>
      <xs:documentation xml:lang="en">List of data points that define the characteristic curve.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DataPoint" type="cfg:DataPointLengthVelType" minOccurs="2" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="Units" fixed="mm and mm/s" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="LaserSwitchConfigType">
    <xs:annotation>
      <xs:documentation xml:lang="EN">Laser Switch Settings</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="LaserPreTriggerTime" type="cfg:TimeShift" default="0">
         <xs:annotation>
            <xs:documentation xml:lang="en">Adding a certain time upfront the Laser On gate signal (i.e. shifts the laser on event forward in time while keeping the laser off event).</xs:documentation>
         </xs:annotation>
      </xs:element>
      <xs:element name="LaserSwitchOffsetTime" type="cfg:TimeShiftMax1s" default="-20e-6">
          <xs:annotation>
            <xs:documentation xml:lang="en">Moving the Laser Off gate signal back in time for positive values. Negative values can be used to compensate for laser response times.</xs:documentation>
          </xs:annotation>
      </xs:element>
      <xs:element name="LaserMinOffTime" type="cfg:DurationMin1over64us" default="1e-6">
        <xs:annotation>
          <xs:documentation xml:lang="en">Minimum duration the laser must be switched off in between two disjointed mark elements. Jump time will be increased to this minimum if necessary.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
    <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:simpleType name="FactorType">
    <xs:restriction base="xs:double">
      <xs:minInclusive value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CharacteristicCurveVelocityFactorType">
    <xs:annotation>
      <xs:documentation xml:lang="en">The VelocityFactor is a scaling factor that is multiplied to the output value of that specific Channel dependent on the spot velocity of the laser beam.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DataPoint" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="Velocity" type="cfg:NonNegDouble" use="required" />
          <xs:attribute name="Factor" type="cfg:FactorType" use="required" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Enabled" type="xs:boolean" use="required" />
    <xs:attribute name="VelocityUnit" fixed="mm/s" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="CharacteristicCurveRadiusFactorType">
    <xs:annotation>
      <xs:documentation xml:lang="en">The RadiusFactor is a scaling factor that is multiplied to the output value of that specific Channel dependent on the excursion radius of the laser beam.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DataPoint" minOccurs="0" maxOccurs="unbounded">
        <xs:complexType>
          <xs:attribute name="Radius" type="cfg:NonNegDouble" use="required" />
          <xs:attribute name="Factor" type="cfg:FactorType" use="required" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Enabled" type="xs:boolean" use="required" />
    <xs:attribute name="RadiusUnit" fixed="mm" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="LaserControlFlagsType">
    <xs:all>
      <xs:element minOccurs="0" name="LaserDisable" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines whether the laser control signals are active or deactivated.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="PulseSwitchSetting" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines whether the Laser1 and Laser2 signals are cut off after the Laser on phase.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="LaserSignalPhaseShift" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines whether the Laser1 and Laser2 signals are phase shifted.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="LaserOnSignalActiveLow" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines whether the LaserOn signal is low active (true).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Laser1Laser2SignalActiveLow" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines whether the Laser1 / Laser2 signals are low active (true).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="LaserPulsesAtRisingEdge" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">[Experimental] Defines if laser pulses for output synchronisation is at rising or falling edge. If true then it is on rising edge.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="OutputSynchronizationOn" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="EN">[Experimental] Defines whether the RTC6 should synchronize on an external signal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      
    </xs:all>
  </xs:complexType>
  <xs:complexType name="ChannelConfigurationTypeShift">
          <xs:simpleContent>
            <xs:extension base="xs:double">
              <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
  <xs:complexType name="ChannelConfigurationAnalogOutType">
    <xs:all>
      <xs:element name="Shift" type="cfg:ChannelConfigurationTypeShift" minOccurs="0" default="0.0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Additional time shift. Currently only n*10us, i.e. multiples of 10us are supported.</xs:documentation>
          </xs:annotation>
      </xs:element>
      <xs:element name="RadiusFactor" type="cfg:CharacteristicCurveRadiusFactorType" />
      <xs:element name="VelocityFactor" type="cfg:CharacteristicCurveVelocityFactorType" />
    </xs:all>
    <xs:attribute name="DefaultOutput" type="cfg:UnitIntervalType" use="required">
     <xs:annotation>
        <xs:documentation xml:lang="EN">Default output being multiplied to the maximum value of 10V. The factors caused by the characteristic curves of the respective laser control channel are additionionally multiplied to that value.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Format" fixed="Factor" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="ChannelConfigurationTimeType">
    <xs:all>
      <xs:element name="Shift" type="cfg:ChannelConfigurationTypeShift" minOccurs="0" default="0.0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Additional time shift. Currently only n*10us, i.e. multiples of 10us are supported.</xs:documentation>
          </xs:annotation>
      </xs:element>
      <xs:element name="RadiusFactor" type="cfg:CharacteristicCurveRadiusFactorType" />
      <xs:element name="VelocityFactor" type="cfg:CharacteristicCurveVelocityFactorType" />
    </xs:all>
    <xs:attribute name="DefaultOutput" type="cfg:NonNegDouble" use="required">
      <xs:annotation>
        <xs:documentation xml:lang="EN">Default output which is multiplied with the factors defined by the characteristic curves of the respective laser control channel.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Unit" fixed="s" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="ChannelConfigurationDistanceType">
    <xs:all>
      <xs:element name="Shift" type="cfg:ChannelConfigurationTypeShift" minOccurs="0" default="0.0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Additional time shift. Currently only n*10us, i.e. multiples of 10us are supported.</xs:documentation>
          </xs:annotation>
      </xs:element>
      <xs:element name="RadiusFactor" type="cfg:CharacteristicCurveRadiusFactorType" />
      <xs:element name="VelocityFactor" type="cfg:CharacteristicCurveVelocityFactorType" />
    </xs:all>
    <xs:attribute name="DefaultOutput" type="cfg:NonNegDouble" use="required">
       <xs:annotation>
        <xs:documentation xml:lang="EN">Default spot distance in [mm]. The factors caused by the characteristic curves of the respective laser control channel are being multiplied to the spot velocity information, i.e. causing an inverse scaling of the actual spot distance (but linear to the deposed laser energy).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="Unit" fixed="mm" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:simpleType name="ChannelType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="AnalogOut1" />
      <xs:enumeration value="AnalogOut2" />
      <xs:enumeration value="PulseLength" />
      <xs:enumeration value="HalfPeriod" />
      <xs:enumeration value="SpotDistance" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ActiveChannelType">
    <xs:sequence>
      <xs:element name="Channel" type="cfg:ChannelType" minOccurs="0" maxOccurs="2" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LaserConfigType">
    <xs:all>
      <xs:element name="LaserMode" default="0" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the RTC Laser Mode of the syncAXIS control instance.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:nonNegativeInteger">
            <xs:maxInclusive value="6" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LaserPortCfg" type="cfg:LaserPortsConfigType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Sets the output ports of the various laser signals.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaserOutput" type="cfg:PulseConfigType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the pulse train of the laser control output during 'Laser is on' which is dependent on the Laser Mode.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaserStandby" type="cfg:PulseConfigType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the pulse train of the laser control output during the 'laser is off which dependent on the Laser Mode.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="QSwitchDelay" type="cfg:Duration" default="0" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the delay of the Q-Switch signal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FPulseKillerLength" type="cfg:Duration" default="0" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the length of the FirstPulseKiller signal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaserControlFlags" type="cfg:LaserControlFlagsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Defines the laser control flags enabled.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AutomaticLaserControl">
        <xs:annotation>
           <xs:documentation xml:lang="en">All automatic laser control settings are defines within this parent tag.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:all>
            <xs:element name="ActiveChannel" type="cfg:ActiveChannelType">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines which outputs (analog outputs, spot distance and laser signals) are adapted (based on radius and velocity) automatically via channels.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AnalogOut1" type="cfg:ChannelConfigurationAnalogOutType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines how the voltage of the Analog Out 1 port is adapted based on radius and velocity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="AnalogOut2" type="cfg:ChannelConfigurationAnalogOutType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines how the voltage of the Analog Out 2 port is adapted based on radius and velocity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PulseLength" type="cfg:ChannelConfigurationTimeType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines how the pulse length is adapted based on radius and velocity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="HalfPeriod" type="cfg:ChannelConfigurationTimeType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines how the pulse half period is adapted based on radius and velocity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SpotDistance" type="cfg:ChannelConfigurationDistanceType" minOccurs="0">
              <xs:annotation>
                <xs:documentation xml:lang="en">Defines how the spot distance is adapted based on radius and velocity.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:all>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="MarkConfigType">
    <xs:annotation>
      <xs:documentation xml:lang="EN">Mark Configuration Settings</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="JumpSpeed" type="cfg:Velocity" default="400">
        <xs:annotation>
          <xs:documentation xml:lang="en">Maximum velocity per axis of the virtual laser focal point (if the laser was on) relative to the surface of the material being processed during a jump.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkSpeed" type="cfg:Velocity" default="400">
        <xs:annotation>
          <xs:documentation xml:lang="en">Desired velocity of the laser focal point along a mark vector relative to the surface of the material being processed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MinimalMarkSpeed" type="cfg:ZeroVelocity" default="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Sets a lower limit for mark velocity reduction when calculating blend arc between adjacent mark vectors.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LaserSwitchConfig" type="cfg:LaserSwitchConfigType" />
    </xs:all>
    <xs:attribute name="VelocityUnit" fixed="mm/s" type="xs:string" use="optional" />
  </xs:complexType>
  <xs:complexType name="GeometryConfigType">
      <xs:annotation>
      <xs:documentation xml:lang="EN">Geometry Configuration Settings</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element name="MaxBlendRadius" type="cfg:Length" default="1.0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Maximum radius of range circle for blending between two adjacent mark vectors. Limited by the minimum of half the length of the two adjacent vectors</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ApproxBlendLimit" type="cfg:Length" default="0.5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Maximum allowed minimum distance between the blend arc of two adjacent mark vectors and their connecting vertex.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BlendMode" default="MinimalBlending">
        <xs:annotation>
          <xs:documentation xml:lang="en">Deactivated = No blendings are used instead a skywriting jump will be inserted between adjacent vectors; VariableBlending = Blending will be adjusted by velocity constrained by MaxVectorBlendRadius and VectorApproxLimit; MinimalBlending  = If true trajectory planning will use blends between adjacent vectors with minimal distance to their connecting vertex; FixedBlending = Deprecated. Converts consecutive lines into a composite spline.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Deactivated" />
            <xs:enumeration value="VariableBlending" />
            <xs:enumeration value="MinimalBlending" />
            <xs:enumeration value="FixedBlending" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AutoCyclicGeometry" type="xs:boolean" default="false">
        <xs:annotation>
          <xs:documentation xml:lang="en">If set "true" and is applied if either BlendMode is in "FixedBlending" or SplineMode is "Interpolating" or "Approximating". Treats closed splines as cyclic splines (all edges are smooth). Deprecated setting, it is recommended not to change this.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SplineConversionLengthLimit" type="cfg:Length" default="0.5">
        <xs:annotation>
          <xs:documentation xml:lang="en">Maximum length of vectors to be treated as spline points. Deprecated setting, it is recommended not to change this.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SplineMode" default="Deactivated">
        <xs:annotation>
          <xs:documentation xml:lang="en">Configures how polygons are converted to a spline path: Deactivated = polygons do not get converted into splines; Interpolating = every polygon point is interpolated; Approximating = polygon is approximated using the ApproxLimit parameter. Deprecated setting, it is recommended to leave in Deactivated.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Deactivated" />
            <xs:enumeration value="Interpolating" />
            <xs:enumeration value="Approximating" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="DynamicReductionFunctionType">
    <xs:sequence minOccurs="0" maxOccurs="unbounded">
      <xs:element name="DynamicReductionFunction" type="cfg:CharacteristicCurveLengthVelType">
        <xs:annotation>
          <xs:documentation xml:lang="EN">Velocity reduction curve. It is possible to provide a list of curves, which can be selected via API command.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="HeuristicConfigType">
    <xs:all>
      <xs:annotation>
        <xs:documentation xml:lang="en">Having long vectors exceeding the length of half the scan head field of view with high velocities, the stage might experience a higher dynamic load than it is capable of. For those cases, a function in form of a characteristic curve of DataPoints containing length and velocity information can be defined to reduce the combined velocity accordingly.</xs:documentation>
      </xs:annotation>
      <xs:element name="DynamicReductionFunctions" type="cfg:DynamicReductionFunctionType" minOccurs="0" />
      <xs:element name="HeuristicForJumpsOnly" type="xs:boolean" minOccurs="0" default="false" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="TrajectoryConfigType">
    <xs:all>
      <xs:element name="MarkConfig" type="cfg:MarkConfigType" />
      <xs:element name="GeometryConfig" type="cfg:GeometryConfigType" />
      <xs:element name="VectorResolution" type="cfg:NonZeroLength" default="0.02">
        <xs:annotation>
          <xs:documentation xml:lang="en">Sets a global minimum length to be used when identifying whether two points are equal.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>

  


  
  
